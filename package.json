{
  "name": "react-series",
  "description": "Colletion of small React apps",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "build": "cd client && yarn build",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "prod": "set NODE_ENV=production&& node server"
  },
  "engines": {
    "npm": "6.1.0",
    "node": "10.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.2",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "dotenv": "^6.0.0"
  }
}
